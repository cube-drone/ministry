b:
	docker compose up -d
	docker logs --follow ministry_ministry_1
build:
	docker compose up -d
	docker logs --follow ministry_ministry_1

c:
	docker compose down
clean:
	docker compose down

s:
	docker container restart ministry_ministry_1 && docker logs --follow ministry_ministry_1
start:
	docker container restart ministry_ministry_1 && docker logs --follow ministry_ministry_1